#include <stdio.h>
#include <stdlib.h>

#include "SocketLibrary.h"

#define LOCALHOST "127.0.0.1"
#define PUERTO "7171"

int main(void) {

	int s= getConnectedSocket(LOCALHOST,PUERTO);
	char mensaje[5];
	strcpy(mensaje,"Hola");
	lSend2(s,mensaje,sizeof(mensaje));

	return EXIT_SUCCESS;
}

void lSend2(int sender, void* msg, int len){
	_sendHeader(sender,1,len);//tipo de proceso hardcodeado, hay que ver de donde pija se saca
	//internalSend(sender,msg,len);
}
